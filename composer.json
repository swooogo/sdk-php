{
  "name": "swoogo/authorizenet",
  "type": "yii2-extension",
  "description": "Official PHP SDK for Authorize.Net",
  "keywords": [
    "authorizenet",
    "authorize.net",
    "payment",
    "ecommerce"
  ],
  "license": "proprietary",
  "homepage": "http://developer.authorize.net",
  "require": {
    "php": ">=5.6",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "replace": {
    "authorizenet/authorizenet": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpmd/phpmd": "~2.0"
  },
  "autoload": {
    "classmap": [
      "lib"
    ]
  },
  "extra": {
    "bootstrap": "net\\authorize\\util\\Helpers"
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  }
}
